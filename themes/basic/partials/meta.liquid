<meta name="description" content="{{ site.description }}">
<meta property="og:site_name" content="{{ site.title }}" />
<meta property="og:title" content="{{ page.url }}" />
<meta property="og:description" content="{{ date }}" />
<meta property="og:url" content="{{ site.url }}{{ page.url }}" />
<meta property="og:image" content="{{site.url}}/images/card.jpg" />

<meta name="twitter:card" content="summary" />
<meta name="twitter:site" content="{{ site.url }}" />
<meta name="twitter:creator" content="@vikramrojo" />
<meta name="twitter:title" content="{{ page.url }}" />
<meta name="twitter:description" content="{{ date }}" />
<meta name="twitter:image" content="{{site.url}}/images/card.jpg" />